ScreenManager:
        Login_scr:
        Home_scr:
        Hall_selection:
        Seat_selection1:
<Login_scr>:
        name:"login"
        Image:
            #C://Users//asus//Documents//appsdevlopment using python//
                source:"liph.png"
                allow_stretch:True
                keep_ratio:False
        MDLabel:
                text:"WELCOME"
                color:"#008080"
                halign:"center"
                pos_hint:{'center_y':0.8}
                font_size:"48dp"
        MDTextField:
                id:username
                hint_text:"Username"
                text_color:"Blue"
                helper_text:"expample@tmu.ac.in"
                helper_text_mode:"on_focus"
                icon_right:"account"
                helper_text_size:20
                icon_right_color:"#FFA500"
                pos_hint:{'center_x':0.5,'center_y':0.645}
                size_hint_x:None
                width:500
                font_size:"25dp"
                line_color_normal:"#FFFF00"
                line_color_focus:"#FFA500"
                multiline:False
                mode:"rectangle"
                on_text:self.text = self.text.replace(" ","")
                write_tab:False


        MDTextField:
                id:passwd
                hint_text:"Password"
                helper_text_mode:"on_focus"
                icon_right:"eye-off"
                password:True
                pos_hint:{'center_x':0.5,'center_y':0.5}
                size_hint_x:None
                width:500
                font_size:"25dp"
                line_color_normal:"#FFFF00"
                line_color_focus:"#FFA500"
                mode:"rectangle"
                multiline:False
                on_text:self.text = self.text.replace(" ","")
                write_tab:False

        BoxLayout:
                orientation:"horizontal"
                spacing:"5dp"

                MDCheckbox:
                        id:cb
                        size_hint:None, None
                        width:"30dp"
                        height:"30dp"
                        pos_hint:{'center_x':0.3,'center_y':0.4}
                        on_press:
                                passwd.password=False if passwd.password == True else True
                                passwd.icon_right = "eye" if passwd.icon_right == "eye-off" else "eye-off"

                MDLabel:
                        text: "[ref=Show Password]Show Password[/ref]"
                        markup:True
                        halign:"left"
                        pos_hint:{'center_y':0.4}
                        font_color:"#FFA500"
                        on_ref_press:
                                cb.active =False if cb.active==True else True
                                passwd.password=False if passwd.password == True else True
                                passwd.icon_right = "eye" if passwd.icon_right == "eye-off" else "eye-off"

        MDRectangleFlatButton:
                text : "SIGN IN"
                pos_hint:{'center_x':0.6,'center_y':0.3}
                md_bg_color:"#A52A2A"
                font_size:"22dp"
                on_press:app.get_data()

<Home_scr>:
        name:"home"
        Image:
            #C://Users//asus//Documents//appsdevlopment using python//
                source:"liph.png"
                allow_stretch:True
                keep_ratio:False

<Hall_selection>:
        name:"hall_selection"

        Image:
            #C://Users//asus//Documents//appsdevlopment using python//
                source:"liph.png"
                allow_stretch:True
                keep_ratio:False
        MDBoxLayout:
                orientation:"vertical"
                MDTopAppBar:
                        title:"Number of Halls"
                        elevation:3

                MDScrollView:
                        pos_hint:{"center_x":0.55,"center_y":0.3}
                        MDList:
                                id:container






